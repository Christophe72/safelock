generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model CableSelection {
  id        Int      @id @default(autoincrement())
  type      String
  calibre   String
  distance  Int
  section   Int
  note      String?
  createdAt DateTime @default(now())
}
